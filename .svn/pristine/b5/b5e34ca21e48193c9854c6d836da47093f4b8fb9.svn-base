package graphics;

import javax.swing.ImageIcon;
import javax.swing.JWindow;

import components.ImagePanel;

public class FenetreIntroduction extends JWindow {
	private static final long serialVersionUID = 1L;

	public FenetreIntroduction() {
		setSize(750, 469);//On lui donne une taille pour qu'on puisse la voir
		setLocationRelativeTo(null);

		ImagePanel image_panel = new ImagePanel(new ImageIcon(getClass().getResource("fenetre_introduction1.jpg")).getImage());
		add(image_panel);
		
		setVisible(true);//On la rend visible
	}

}
